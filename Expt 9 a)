#include <iostream>
#include <fstream>
using namespace std;

int main() {
    ifstream fin("First.txt", ios::in);
    ofstream fout("Second.txt", ios::out);

    if (!fin) {
        cout << "Error: Cannot open First.txt" << endl;
        return 1;
    }

    char ch;
    while (fin.get(ch))
        fout.put(ch);

    cout << "File copied successfully." << endl;
    fin.close();
    fout.close();
    return 0;
}
