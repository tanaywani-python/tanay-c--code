#include <iostream>
using namespace std;

class Area {
public:
    void calcArea(float side) {
        cout << "Area of Classroom (Square) = " << side * side << endl;
    }

    void calcArea(float length, float breadth) {
        cout << "Area of Laboratory (Rectangle) = " << length * breadth << endl;
    }
};

int main() {
    Area a;
    a.calcArea(5.5);          
    a.calcArea(10.5, 7.5);    
    return 0;
}
